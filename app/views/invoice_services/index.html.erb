<h1>Invoice Services</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoice_services.each do |invoice_service| %>
      <tr>
        <td><%= link_to 'Show', invoice_service %></td>
        <td><%= link_to 'Edit', edit_invoice_service_path(invoice_service) %></td>
